{
  "name": "@react-navigation/example",
  "description": "Demo app to showcase various functionality of React Navigation",
  "version": "5.1.0",
  "private": true,
  "main": "./App.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "server": "nodemon -e '.js,.ts,.tsx' --exec \"babel-node -i '/node_modules[/\\](?react-native)/' -x '.web.tsx,.web.ts,.web.js,.tsx,.ts,.js' --config-file ./server/babel.config.js server\"",
    "fingerprint:android": "npx expo-updates fingerprint:generate --platform android | jq -r '.hash' | xargs -n 1 echo 'fingerprint:'",
    "fingerprint:ios": "npx expo-updates fingerprint:generate --platform ios | jq -r '.hash' | xargs -n 1 echo 'fingerprint:'",
    "build:debug:android": "npx expo prebuild --yarn && cd android && ./gradlew assembleDebug -DtestBuildType=debug -Dorg.gradle.jvmargs=-Xmx4g",
    "build:debug:ios": "npx expo prebuild --yarn && xcodebuild ONLY_ACTIVE_ARCH=YES -workspace ios/ReactNavigation.xcworkspace -UseNewBuildSystem=YES -scheme ReactNavigation -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -quiet",
    "e2e:native": "maestro test -e APP_ID=org.reactnavigation.example -e APP_SCHEME=rne:// e2e/maestro",
    "e2e:web": "npx playwright test --config=e2e/playwright.config.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-masked-view/masked-view": "0.3.2",
    "color": "^4.2.3",
    "expo": "^53.0.5",
    "expo-asset": "~11.1.4",
    "expo-blur": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.12",
    "koa": "^3.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-edge-to-edge": "^1.6.0",
    "react-native-gesture-handler": "~2.25.0",
    "react-native-pager-view": "6.7.1",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/node": "^7.27.1",
    "@babel/plugin-transform-strict-mode": "^7.27.1",
    "@playwright/test": "^1.52.0",
    "@types/cheerio": "^0.22.35",
    "@types/koa": "^2.15.0",
    "@types/mock-require": "^3.0.0",
    "@types/react": "~19.0.10",
    "@types/react-dom": "~19.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~13.0.0",
    "cheerio": "^1.0.0",
    "expect-type": "^1.2.1",
    "expo-dev-client": "~5.1.7",
    "mock-require-assets": "^0.0.3",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^7.0.2",
    "react-native-builder-bob": "^0.40.7",
    "react-native-monorepo-config": "^0.1.7",
    "react-test-renderer": "19.0.0",
    "serve": "^14.2.4",
    "typescript": "^5.8.3",
    "yaml": "^2.7.1"
  }
}
